{
  "name": "sd-webui-api",
  "version": "1.0.0",
  "description": "A Typescript API client for AUTOMATIC111/stable-diffusion-webui",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
 },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datamonet/sd-webui-api.git"
  },
  "keywords": ["stable diffusion", "webui", "api"],
  "author": "DataMonet LLC",
  "license": "MIT License",
  "bugs": {
    "url": "https://github.com/datamonet/sd-webui-api/issues"
  },
  "homepage": "https://github.com/datamonet/sd-webui-api#readme",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.3"
  },
  "files": [
    "lib/**/*"
  ]

}
